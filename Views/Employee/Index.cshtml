@model IEnumerable<employeeName.Models.DBEntities.Employee>

<h2>Employee List</h2>
<table>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <!-- Add other columns as needed -->
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.FirstName @employee.LastName</td>

            <td>@employee.Email</td>
            <!-- Add other columns as needed -->
        </tr>
    }
</table>
